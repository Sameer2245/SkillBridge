# Frontend Production Environment Variables
# ==========================================

# API Base URL (Your backend domain)
REACT_APP_SERVER_URL=https://skillbridge-production-53c7.up.railway.app

# Stripe Publishable Key (Production)
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_live_your_production_stripe_publishable_key

# App Configuration
REACT_APP_APP_NAME=SkillBridge
REACT_APP_APP_VERSION=1.0.0

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_ERROR_TRACKING=true

# Social Login (if implemented)
REACT_APP_GOOGLE_CLIENT_ID=your_google_client_id
REACT_APP_FACEBOOK_APP_ID=your_facebook_app_id

# External Services
REACT_APP_CLOUDINARY_CLOUD_NAME=your_production_cloud_name

# Build Configuration
GENERATE_SOURCEMAP=false
BUILD_PATH=build
