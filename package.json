{
  "name": "skillbridge",
  "version": "1.0.0",
  "description": "SkillBridge - A comprehensive freelance marketplace built with React, Node.js, and MongoDB",
  "main": "server/server.js",
  "scripts": {
    "setup": "node setup.js",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "cd server && npm run dev",
    "client:dev": "cd client && npm start",
    "server:start": "cd server && npm start",
    "client:build": "cd client && npm run build",
    "install:all": "npm install && cd server && npm install && cd ../client && npm install",
    "test": "cd client && npm test",
    "build": "cd client && npm run build"
  },
  "keywords": [
    "freelance",
    "marketplace",
    "react",
    "nodejs",
    "mongodb",
    "stripe",
    "socket.io",
    "skillbridge"
  ],
  "author": "SkillBridge Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "express-list-endpoints": "^7.1.1",
    "jsonwebtoken": "^9.0.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/skillbridge.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/skillbridge/issues"
  },
  "homepage": "https://github.com/your-username/skillbridge#readme"
}
